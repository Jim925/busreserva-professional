  "author": {
    "name": "BusReserva Ultra Team",
    "email": "dev@busreserva.com",
    "url": "https://busreserva.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/busreserva/ultra/issues"
  },
  "homepage": "https://busreserva.com",
  "config": {
    "port": 3000,
    "database": "bus_reservation_ultra",
    "redis_url": "redis://localhost:6379",
    "jwt_secret": "ultra_secret_key_2024",
    "email_service": "gmail",
    "upload_limit": "10mb",
    "rate_limit": 100,
    "session_secret": "ultra_session_secret_2024"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md,css,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "nyc": {
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "exclude": [
      "test/**",
      "coverage/**",
      "node_modules/**"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "server-ultra.js",
      "routes/",
      "middleware/",
      "models/",
      "services/",
      "utils/"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "public/",
      "uploads/",
      "logs/",
      "coverage/"
    ],
    "delay": 1000
  }
}