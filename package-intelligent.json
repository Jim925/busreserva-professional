{
  "name": "busreserva-pro-intelligent",
  "version": "2.0.0",
  "description": "Sistema Inteligente de Reserva de Autobuses con IA, Machine Learning y Funcionalidades Avanzadas",
  "main": "server-intelligent.js",
  "scripts": {
    "start": "node server-intelligent.js",
    "dev": "nodemon server-intelligent.js",
    "smart-start": "node server-intelligent.js",
    "install-db": "mysql -u root -p < database.sql",
    "test": "jest",
    "lint": "eslint .",
    "build": "webpack --mode production",
    "analyze": "node scripts/analyze-performance.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "node-cache": "^5.1.2",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "autobuses",
    "reservas",
    "sistema-inteligente",
    "mysql",
    "express",
    "nodejs",
    "jwt-auth",
    "machine-learning",
    "ai-powered",
    "smart-booking",
    "bus-reservation",
    "transport-management",
    "intelligent-system",
    "predictive-analytics",
    "real-time",
    "microservices"
  ],
  "author": {
    "name": "BusReserva Pro Team",
    "email": "admin@busreservapro.com",
    "url": "https://busreservapro.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/busreservapro/intelligent-system.git"
  },
  "bugs": {
    "url": "https://github.com/busreservapro/intelligent-system/issues"
  },
  "homepage": "https://github.com/busreservapro/intelligent-system#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "config": {
    "port": 3000,
    "database": "bus_reservation",
    "jwt_expiry": "24h",
    "cache_ttl": 300,
    "rate_limit": 100
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error"
    }
  }
}